procedure Simple is
	a, A, r5i8 : INTEGER;
	BQ:BOOLEAN;
	Z, e3 : array [1..4] of INTEGER;
begin
	A := 34; a := 1;
	r5i8 := (A + 23) / (A * 4);
	for I in a-1..A+2 loop
		if (A - a < 0) then
			BQ := r5i8 >= a+1;
		elsif (r5i8 >= A + a) then
			a := Z[3]-12;
			exit when a_3 > 100;
		else
			r5i8 := (A - 45 * r5i8) * (a + A -2) / (r5i8 - 2) / 5;
		end if;
		e3[I] := Z[2 * I - 3] *23;
	end loop;
	a := A+r5i8;
end;
